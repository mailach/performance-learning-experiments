name: pim-pipeline

docker_env:
  image: mailach/pim-pipeline
  environment:
    [
      "MLFLOW_TRACKING_URI",
      "MLFLOW_TRACKING_USERNAME",
      "MLFLOW_TRACKING_PASSWORD",
    ]

entry_points:
  evaluation:
    parameters:
      learning_run_id: learning_run_id
    command: "python evaluation.py --learning_run_id={learning_run_id}"
